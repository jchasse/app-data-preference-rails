<%= form_for @account do |f| %>

  <div>
    <%= f.label :org_name %>
    <br>
    <%= f.text_field :org_name %>
  </div>

  <div>
    <%= f.label :website %>
    <br>
    <%= f.text_field :website %>
  </div>

  <div>
    <%= f.label :toll_free_number %>
    <br>
    <%= f.text_field :toll_free_number %>
  </div>

    <%=f.fields_for :digitalprints do |d|%>
        <%=d.label :kind%>
        <%=d.text_field :kind %><br>
    <%end%>

  <div>
    <br/>
    <%= f.submit %>
  </div>

<% end %>